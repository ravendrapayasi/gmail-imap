{
	"name": "anod/gmail-imap",
	"description": "API to work with Gmail using IMAP built on top of Zend Imap Library",
	"keywords": ["gmail", "imap"],
	"homepage": "https://github.com/ravendrapayasi/gmail-imap",
	"authors": [
        {
            "name": "Ravendra Payasi",
            "email": "ravendrapayasi@gmail.com"
        }
     ],
	"license": "MIT",
    "require": {
        "php": "^7.2",
        "zendframework/zend-mail": "2.10.*",
		"psr/log": "^1.1"
    },
	"require-dev": {
        "phpunit/phpunit": "^8",
		"google/apiclient": "^2.0",
		"squizlabs/php_codesniffer": "^3.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
	],  
	"autoload": {
        "psr-0": {"Ravendra": "src/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    } 
}
